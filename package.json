{
  "name": "nextjs-commerce",
  "version": "1.0.0",
  "scripts": {
    "postdeploy": "yarn build",
    "dev": "NODE_OPTIONS='--inspect' next dev -p 4000",
    "build": "next build",
    "start": "next start -p 4000",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "lint": "next lint",
    "prettier-fix": "prettier --write .",
    "find:unused": "npx next-unused",
    "generate": "graphql-codegen",
    "generate:definitions": "node framework/bigcommerce/scripts/generate-definitions.js",
    "flatfile:develop": "npx flatfile develop ./flatfile/index.js",
    "flatfile:deploy": "npx flatfile deploy ./flatfile/index.js",
    "type-check": "tsc --noEmit"
  },
  "sideEffects": false,
  "license": "MIT",
  "dependencies": {
    "@flatfile/api": "^1.8.4",
    "@flatfile/hooks": "^1.4.0",
    "@flatfile/listener": "^1.0.2",
    "@flatfile/plugin-record-hook": "^1.5.1",
    "@flatfile/plugin-xlsx-extractor": "^1.11.9",
    "@flatfile/react": "^7.9.1",
    "@sendgrid/mail": "^7.6.0",
    "@stripe/react-stripe-js": "^1.7.0",
    "@stripe/stripe-js": "^1.24.0",
    "@vercel/fetch": "^6.1.0",
    "autoprefixer": "^10.2.6",
    "axios": "^0.24.0",
    "body-scroll-lock": "^3.1.5",
    "classnames": "^2.3.1",
    "cookie": "^0.4.1",
    "handlebars": "^4.7.7",
    "html-pdf": "^3.0.1",
    "html-react-parser": "^1.3.0",
    "immutability-helper": "^3.1.1",
    "js-cookie": "^2.2.1",
    "json2csv": "^5.0.7",
    "keen-slider": "^5.5.1",
    "klaviyo-api": "^8.0.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.29.2",
    "next": "^13.5.1",
    "next-seo": "^4.26.0",
    "next-themes": "^0.0.14",
    "nextjs-cors": "^2.1.1",
    "parse-address": "^1.1.2",
    "postcss": "^8.3.11",
    "postcss-nesting": "^12.0.1",
    "puppeteer": "^21.5.0",
    "react": "^18.2.0",
    "react-calendar": "^3.5.0",
    "react-cool-inview": "^2.0.8",
    "react-datepicker": "^4.3.0",
    "react-device-detect": "^2.1.2",
    "react-dom": "^18.2.0",
    "react-facebook-login": "^4.1.1",
    "react-google-autocomplete": "^2.6.1",
    "react-google-login": "^5.2.2",
    "react-google-recaptcha": "^2.1.0",
    "react-image-gallery": "^1.2.7",
    "react-imask": "^6.4.2",
    "react-markdown": "^7.1.0",
    "react-merge-refs": "^1.1.0",
    "react-paginate": "^7.1.5",
    "react-select": "^5.0.0",
    "rehype-raw": "6.0.0",
    "sass": "^1.43.4",
    "sharp": "^0.30.3",
    "stripe": "^8.207.0",
    "swr": "^0.5.6",
    "tabbable": "^5.2.0",
    "usps-webtools-promise": "^6.1.0",
    "uuidv4": "^6.2.10"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/schema-ast": "^1.18.3",
    "@graphql-codegen/typescript": "^1.22.2",
    "@graphql-codegen/typescript-operations": "^1.18.1",
    "@next/bundle-analyzer": "^10.2.3",
    "@types/body-scroll-lock": "^2.6.1",
    "@types/cookie": "^0.4.0",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^15.12.4",
    "@types/react": "^17.0.33",
    "@types/react-calendar": "^3.4.3",
    "@types/react-datepicker": "^4.1.7",
    "@types/react-facebook-login": "^4.1.4",
    "@types/react-google-recaptcha": "^2.1.5",
    "@types/react-image-gallery": "^1.0.5",
    "@types/react-paginate": "^7.1.1",
    "deepmerge": "^4.2.2",
    "eslint": "^7.31.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "graphql": "^15.7.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.2.6",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.0",
    "typescript": "4.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  }
}
